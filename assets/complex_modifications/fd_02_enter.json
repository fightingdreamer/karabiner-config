{
  "title": "[fd] enter",
  "rules": [
    {
      "description": "[fd] Post right_control when return_or_enter is hold",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "return_or_enter",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_control",
              "lazy": true
            }
          ],
          "to_if_alone": [
            {
              "key_code": "return_or_enter"
            }
          ]
        }
      ]
    }
  ]
}
